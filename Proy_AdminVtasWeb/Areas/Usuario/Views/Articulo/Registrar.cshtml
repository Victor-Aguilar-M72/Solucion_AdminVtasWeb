@model AdminVtasWeb.CapaEntity.Artículo
@*
    @model IEnumerable<Nomina.Areas.nomina.Models.ClavePresupuestal>
*@
@{
    ViewData["Title"] = "Registro de Articulo";
}
<h1>Registro de Articulo</h1>
<h4>@User.Identity?.Name</h4>
<form method="post" class="form-floating">
    <div class="form-group">
        <label asp-for="Codigo"></label>
        <input class="form-control" asp-for="Codigo" />
    </div>
    <div class="form-group">
        <label asp-for="Descripcion"></label>
        <input class="form-control" asp-for="Descripcion" />
    </div>
    <div class="form-group">
        <label asp-for="Precio"></label>
        <input class="form-control" asp-for="Precio" />
    </div>
    <div class="form-group">
        <label asp-for="Imagen"></label>
        <input class="form-control" asp-for="Imagen" />
    </div>
    <div class="form-group">
        <label asp-for="stock"></label>
        <input class="form-control" asp-for="stock" />
    </div>
    <br />
    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Guardar</button>
    </div>

</form>

